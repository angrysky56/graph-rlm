{
  "mcpServers": {
    "docker-mcp": {
      "command": "uvx",
      "args": ["docker-mcp"]
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--browser", "chrome"]
    },
    "gitmcp": {
      "command": "npx",
      "args": ["mcp-remote", "https://gitmcp.io/docs"]
    },
    "notebooklm": {
      "command": "npx",
      "args": ["notebooklm-mcp@latest"]
    },
    "desktop-commander": {
      "command": "npx",
      "args": ["-y", "@wonderwhy-er/desktop-commander@latest"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "chatdag": {
      "command": "uv",
      "args": [
        "--directory",
        "/home/ty/Repositories/ai_workspace/mcp_coordinator/mcp_servers/chat-dag",
        "run",
        "chatdag",
        "mcp"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1",
        "LOG_LEVEL": "WARNING"
      }
    },
    "reflective-agent-architecture": {
      "command": "uv",
      "args": [
        "--directory",
        "/home/ty/Repositories/ai_workspace/mcp_coordinator/mcp_servers/reflective-agent-architecture",
        "run",
        "raa-server"
      ],
      "env": {
        "LOG_LEVEL": "WARNING",
        "PYTHONUNBUFFERED": "1"
      }
    }
  }
}
